{
  "name": "Svelte-Webcomponents",
  "version": "1.0.0",
  "description": "Boilerplate for developing web components in svelte, using nested components, passing props, sass and jest setup, eslint and style-lint etc",
  "author": "Talvinder Singh <talvindersingh1992@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "lintcss": "stylelint **/*.{css,svelte}",
    "lintjs": "eslint \"src/**\" ",
    "test": "jest __tests__",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/svelte": "^3.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "jest": "^26.6.3",
    "jest-transform-svelte": "^2.1.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-prettier": "^1.2.0",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.7.0",
    "sirv-cli": "^1.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
